<externaltool id="vi">
    <description>Opens the current file in vi</description>
    <displayname>Edit with vi</displayname>
    <category>Text</category>
    <executable output="showinpane" error="showinpane" modifiesdocument="yes">
        <path>terminator</path>
        <arguments>-f -x vim &quot;%{CurrentDocument:FilePath}&quot; +%{CurrentDocument:Row}</arguments>
        <workingdirectory>%{CurrentDocument:Path} </workingdirectory>
    </executable>
</externaltool>
